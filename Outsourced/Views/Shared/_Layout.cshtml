<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no, user-scalable=0">
    <meta name="description" content="Аутсорсинговая компания по разработке сайтов и desktop приложений">
    <meta name="author" content="FSNO">

    <title>FSNO</title>

    <!-- CSS -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/owl.carousel.min.css" rel="stylesheet">
    <link href="~/css/owl.theme.default.min.css" rel="stylesheet">
    <link href="~/css/animate.min.css" rel="stylesheet">
    <link href="~/css/jquery.pagepiling.css" rel="stylesheet">
    <link href="~/css/jquery.fancybox.min.css" rel="stylesheet">
    <link href="~/css/LineIcons.css" rel="stylesheet">

    <!-- Main CSS -->
    <link href="~/css/style.css" rel="stylesheet">

    <!-- Favicons -->
    <link rel="apple-touch-icon" sizes="180x180" href="~/img/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/img/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/img/favicons/favicon-16x16.png">
    <link rel="manifest" href="~/img/favicons/site.webmanifest">
    <link rel="mask-icon" href="~/img/favicons/safari-pinned-tab.svg" color="#222222">
    <link rel="shortcut icon" href="~/img/favicons/favicon.ico">
    <meta name="msapplication-TileColor" content="#222222">
    <meta name="msapplication-config" content="img/favicons/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">

</head>
<body class="body-copyright-light">
    <div class="preloader">
        <div class="three-bounce">
            <div class="one"></div>
            <div class="two"></div>
            <div class="three"></div>
        </div>
    </div>
<div class="wrapper">
    <header id="header" class="header header-fixed">
        <div class="header-bg"></div>
        <div class="container-fluid clearfix">
            <div class="brand">
                <a href="#Main">
                    <div class="brand-name font-custom">FSNO</div>
                </a>
            </div>

            <button class="nav-toggle-btn a-nav-toggle">
                <span class="nav-toggle">
                    <span class="stick stick-1"></span>
                    <span class="stick stick-2"></span>
                </span>
            </button>

            <div class="header-content d-none d-md-block">
                <div class="row">
                    <div class="col-md-3">
                        <div class="header-tagline">First steps to new opportunities<br /> Аутсорсинговая компания</div>
                    </div>
                    <div class="col-md-9">
                        <div class="header-contacts">
                            <div class="header-contact-item"><a href="tel:+79874568226" class="phone-link">+7 (987) 456-8226</a></div>
                            <div class="header-contact-divider">/</div>
                            <div class="header-contact-item"><a href="mailto:fsno.prog@gmail.com" class="mail-link">fsno.prog@gmail.com</a></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="hide-menu a-nav-toggle"></div>

        <div class="menu">
                
            <nav class="menu-main" id="accordion">
                <ul id="menuMain">
                    <li data-menuanchor="Intro" class="active"><a asp-controller="Home" asp-action="Index" >Главная</a></li>
                    <li data-menuanchor="Design"><a href="#Design">Дизайн</a></li>
                    <li data-menuanchor="WEB"><a href="#WEB">WEB-проекты</a></li>
                    <li data-menuanchor="Desktop"><a href="#Desktop">Desktop-проекты</a></li>
                    <li data-menuanchor="Community"><a href="#Community">Вебинары</a></li>
                    <li data-menuanchor="Mobile"><a href="#Mobile">Мобильная разработка</a></li>
                    <li data-menuanchor="Feedback"><a href="#Feedback">Отзывы</a></li>
                    <li data-menuanchor="Contact"><a href="#Contact">Контакты</a></li>
                </ul>
            </nav>

            <div class="menu-footer">
                <ul class="social social-rounded">
                    <li><a href="https://twitter.com/home"><i class="lni lni-twitter-filled"></i></a></li>
                    <li><a href="https://vk.com"><i class="lni lni-vk"></i></a></li>
                    <li><a href="https://www.instagram.com/"><i class="lni lni-instagram-original"></i></a></li>
                </ul>
                <div class="menu-copyright">&copy; FSNO 2021<br /> Все права защищены</div>
            </div>
        </div>
    </header>
    <div id="content" class="content">
        <div class="homepage-personal a-pagepiling">
            @RenderBody()
        </div>
    </div>
    <footer id="footer" class="footer">
        <ul class="social social-fixed">
            <li><a href="https://twitter.com/home"><i class="lni lni-twitter-filled"></i></a></li>
            <li><a href="https://vk.com"><i class="lni lni-vk"></i></a></li>
            <li><a href="https://www.instagram.com/"><i class="lni lni-instagram-original"></i></a></li>
        </ul>

        <div class="copyright copyright-fixed d-none d-md-block">
            &copy; FSNO 2021<br /> Все права защищены
        </div>
    </footer>
</div>
<script src="https://code.jquery.com/jquery-1.12.4.min.js" integrity="sha256-ZosEbRLbNQzLpnKIkEdrPv7lOy9C27hHQ+Xp8a4MxAQ=" crossorigin="anonymous"></script>
<script>window.jQuery || document.write('<script src="~/js/jquery.min.js"><\/script>')</script>
<script src="~/js/bootstrap.bundle.min.js"></script>
<script src="~/js/owl.carousel.min.js"></script>
<script src="~/js/jquery.pagepiling.min.js"></script>
<script src="~/js/jquery.fancybox.min.js"></script>
<script src="~/js/jquery.validate.min.js"></script>
<script src="~/js/script.js"></script>
</body>
</html>
